{
  "name": "python-vscodium-extension-pack",
  "displayName": "Python Extension Pack",
  "description": "Essential Python development extensions for VSCodium, including language support, debugging, formatting, and testing tools",
  "version": "1.0.0",
  "publisher": "templ-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscodium/python"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "python",
    "data-science",
    "web",
    "development",
    "testing",
    "jupyter",
    "machine-learning",
    "vscodium",
    "python",
    "extension-pack"
  ],
  "extensionPack": [
    "ms-python.python",
    "ms-toolsai.jupyter",
    "ms-python.black-formatter",
    "ms-python.isort",
    "ms-python.pylint",
    "ms-python.flake8",
    "njpwerner.autodocstring",
    "littlefoxteam.vscode-python-test-adapter",
    "ms-toolsai.jupyter",
    "ms-python.pylint",
    "ms-python.flake8",
    "njpwerner.autodocstring",
    "littlefoxteam.vscode-python-test-adapter"
  ],
  "extensionDependencies": [
    "ms-python.python",
    "ms-toolsai.jupyter",
    "ms-python.black-formatter",
    "ms-python.isort",
    "ms-python.pylint",
    "ms-python.flake8",
    "njpwerner.autodocstring",
    "littlefoxteam.vscode-python-test-adapter"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/python.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+b",
        "command": "bookmarks.toggle",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+j",
        "command": "bookmarks.jumpToNext"
      },
      {
        "key": "ctrl+shift+k",
        "command": "bookmarks.jumpToPrevious"
      },
      {
        "key": "ctrl+shift+p",
        "command": "python.execInTerminal",
        "when": "editorLangId &#x3D;&#x3D; python"
      },
      {
        "key": "f5",
        "command": "python.debugInTerminal",
        "when": "editorLangId &#x3D;&#x3D; python"
      },
      {
        "key": "shift+enter",
        "command": "jupyter.execSelectionInteractive",
        "when": "editorLangId &#x3D;&#x3D; python"
      },
      {
        "key": "ctrl+shift+enter",
        "command": "jupyter.runallcellsabove.palette",
        "when": "editorLangId &#x3D;&#x3D; python"
      }
    ],
    "commands": [
      {
        "command": "templ-project.python-vscodium.applySettings",
        "title": "Apply Python Extension Pack Settings"
      },
      {
        "command": "templ-project.python-vscodium.resetSettings",
        "title": "Reset Python Extension Pack Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}