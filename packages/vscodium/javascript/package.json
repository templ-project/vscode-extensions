{
  "name": "tpl-vscodium-javascript",
  "displayName": "JavaScript Extension Pack for Vscodium",
  "description": "Essential JavaScript development environment for VSCodium - runtime-agnostic tools using open-source alternatives",
  "version": "1.2.3",
  "publisher": "templ-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscodium/javascript"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "javascript",
    "es6",
    "frontend",
    "web",
    "development",
    "linting",
    "formatting",
    "extension-pack",
    "development",
    "vscodium",
    "javascript"
  ],
  "extensionPack": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-vscode.js-debug",
    "mgmcdermott.vscode-language-babel",
    "wix.vscode-import-cost",
    "WallabyJs.quokka-vscode",
    "oven.bun-vscode",
    "denoland.vscode-deno"
  ],
  "extensionDependencies": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-vscode.js-debug",
    "mgmcdermott.vscode-language-babel"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+f",
        "command": "prettier.forceFormatDocument",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+e",
        "command": "eslint.executeAutofix",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "templ-project.javascript-vscodium.applySettings",
        "title": "Apply JavaScript Extension Pack for Vscodium Settings"
      },
      {
        "command": "templ-project.javascript-vscodium.resetSettings",
        "title": "Reset JavaScript Extension Pack for Vscodium Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}
