{
  "name": "tpl-vscodium-godot",
  "displayName": "Godot Extension Pack for Vscodium",
  "description": "Essential extensions for Godot game development with GDScript support, debugging, and enhanced productivity tools",
  "version": "1.0.3",
  "publisher": "templ-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscodium/godot"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "godot",
    "gdscript",
    "game development",
    "engine",
    "3d",
    "2d",
    "shaders",
    "extension-pack",
    "development",
    "vscodium",
    "godot"
  ],
  "extensionPack": [
    "geequlim.godot-tools",
    "jjkim.gdscript",
    "alfish.godot-files",
    "arkii.godot-shaders",
    "razoric.gdscript-snippets",
    "wayou.vscode-todo-highlight",
    "2gua.rainbow-brackets"
  ],
  "extensionDependencies": [
    "geequlim.godot-tools",
    "jjkim.gdscript"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "godot",
        "path": "./snippets/godot.json"
      }
    ],
    "keybindings": [
      {
        "key": "f5",
        "command": "godot.debugger.debug",
        "when": "resourceExtname == .gd"
      },
      {
        "key": "ctrl+f5",
        "command": "godot.debugger.debug_without_debugging",
        "when": "resourceExtname == .gd"
      },
      {
        "key": "shift+f5",
        "command": "godot.debugger.stop"
      },
      {
        "key": "ctrl+shift+f5",
        "command": "godot.debugger.restart"
      },
      {
        "key": "f6",
        "command": "godot.open_editor"
      },
      {
        "key": "ctrl+alt+g",
        "command": "godot.list_classes"
      }
    ],
    "commands": [
      {
        "command": "templ-project.godot-vscodium.applySettings",
        "title": "Apply Godot Extension Pack for Vscodium Settings"
      },
      {
        "command": "templ-project.godot-vscodium.resetSettings",
        "title": "Reset Godot Extension Pack for Vscodium Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}
