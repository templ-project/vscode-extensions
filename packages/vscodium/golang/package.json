{
  "name": "tpl-vscodium-golang",
  "displayName": "Go Extension Pack for Vscodium",
  "description": "Essential Go development environment for VSCodium - comprehensive tooling for modern Go development using open-source alternatives",
  "version": "1.3.0",
  "publisher": "templ-project",
  "__configHash": "bc0e7b8de147d3fd8e5cde1624004969396bc5a6e08c8e60a1c4d05edd55f267",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscodium/golang"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "golang",
    "go",
    "backend",
    "development",
    "testing",
    "microservices",
    "extension-pack",
    "development",
    "vscodium",
    "golang"
  ],
  "extensionPack": [
    "golang.go",
    "766b.go-outliner",
    "premparihar.gotestexplorer",
    "davidbarratt.go-fill-struct",
    "karyan40024.gotmpl-syntax-highlighter"
  ],
  "extensionDependencies": [
    "golang.go"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "golang",
        "path": "./snippets/golang.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+t",
        "command": "go.test.package",
        "when": "editorTextFocus && editorLangId == go"
      },
      {
        "key": "ctrl+shift+f",
        "command": "editor.action.formatDocument",
        "when": "editorTextFocus && editorLangId == go"
      },
      {
        "key": "ctrl+shift+r",
        "command": "go.test.file",
        "when": "editorTextFocus && editorLangId == go"
      },
      {
        "key": "f12",
        "command": "editor.action.revealDefinition",
        "when": "editorTextFocus && editorLangId == go"
      },
      {
        "key": "shift+f12",
        "command": "editor.action.goToReferences",
        "when": "editorTextFocus && editorLangId == go"
      }
    ],
    "commands": [
      {
        "command": "templ-project.golang-vscodium.applySettings",
        "title": "Apply Go Extension Pack for Vscodium Settings"
      },
      {
        "command": "templ-project.golang-vscodium.resetSettings",
        "title": "Reset Go Extension Pack for Vscodium Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}
