{
  "name": "tpl-vscodium-typescript",
  "displayName": "TypeScript Extension Pack for Vscodium",
  "description": "Essential TypeScript development environment for VSCodium - comprehensive tooling for type-safe development using open-source alternatives",
  "version": "1.2.0",
  "publisher": "templ-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscodium/typescript"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "typescript",
    "types",
    "development",
    "linting",
    "formatting",
    "intellisense",
    "extension-pack",
    "development",
    "vscodium",
    "typescript"
  ],
  "extensionPack": [
    "ms-vscode.vscode-typescript-next",
    "GregorBiswanger.json2ts"
  ],
  "extensionDependencies": [
    "ms-vscode.vscode-typescript-next",
    "GregorBiswanger.json2ts"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+f",
        "command": "prettier.forceFormatDocument",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+e",
        "command": "eslint.executeAutofix",
        "when": "editorTextFocus"
      },
      {
        "key": "f12",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "templ-project.typescript-vscodium.applySettings",
        "title": "Apply TypeScript Extension Pack for Vscodium Settings"
      },
      {
        "command": "templ-project.typescript-vscodium.resetSettings",
        "title": "Reset TypeScript Extension Pack for Vscodium Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}
