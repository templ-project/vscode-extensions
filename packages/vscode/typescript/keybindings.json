[
  {
    "key": "ctrl+shift+f",
    "command": "prettier.forceFormatDocument",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+e",
    "command": "eslint.executeAutofix",
    "when": "editorTextFocus"
  },
  {
    "key": "f12",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider &amp;&amp; editorTextFocus"
  }
]
