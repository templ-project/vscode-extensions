{
  "name": "tpl-vscode-generic-extended",
  "displayName": "Generic Extended Extension Pack for Vscode",
  "description": "Extended developer tools for API development, containerization, and Git forge integration in VSCode",
  "version": "1.2.3",
  "publisher": "templ-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscode/generic-extended"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "api",
    "swagger",
    "openapi",
    "rest",
    "docker",
    "kubernetes",
    "github",
    "gitlab",
    "bitbucket",
    "forge",
    "extended",
    "extension-pack",
    "development",
    "vscode",
    "generic-extended"
  ],
  "extensionPack": [
    "42crunch.vscode-openapi",
    "humao.rest-client",
    "ms-azuretools.vscode-docker",
    "ms-kubernetes-tools.vscode-kubernetes-tools",
    "tim-koehler.helm-intellisense",
    "github.vscode-pull-request-github",
    "github.vscode-github-actions",
    "jeff-hykin.better-dockerfile-syntax",
    "ipedrazas.kubernetes-snippets",
    "kennylong.kubernetes-yaml-formatter",
    "ms-vscode.vscode-github-issue-notebooks",
    "mhutchie.git-graph",
    "formulahendry.code-runner",
    "SonarSource.sonarlint-vscode"
  ],
  "extensionDependencies": [
    "42crunch.vscode-openapi",
    "humao.rest-client",
    "ms-azuretools.vscode-docker",
    "ms-kubernetes-tools.vscode-kubernetes-tools",
    "tim-koehler.helm-intellisense",
    "github.vscode-pull-request-github",
    "github.vscode-github-actions"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+alt+r",
        "command": "rest-client.request",
        "when": "editorTextFocus && editorLangId == 'http'"
      },
      {
        "key": "ctrl+alt+e",
        "command": "rest-client.request-last",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+d ctrl+shift+b",
        "command": "docker.images.build",
        "when": "resourceExtname == '.dockerfile' || resourceFilename == 'Dockerfile'"
      },
      {
        "key": "ctrl+shift+d ctrl+shift+r",
        "command": "docker.containers.run",
        "when": "focusedView == 'dockerContainers'"
      },
      {
        "key": "ctrl+shift+k ctrl+shift+a",
        "command": "kubernetes.portForward",
        "when": "focusedView == 'extension.vsKubernetesExplorer'"
      },
      {
        "key": "ctrl+shift+k ctrl+shift+l",
        "command": "kubernetes.logs",
        "when": "focusedView == 'extension.vsKubernetesExplorer'"
      },
      {
        "key": "ctrl+shift+g ctrl+shift+p",
        "command": "github.pullRequests.refresh",
        "when": "focusedView == 'github:pullRequests'"
      },
      {
        "key": "ctrl+shift+g ctrl+shift+i",
        "command": "github.pullRequests.openQuery",
        "when": "focusedView == 'github:pullRequests'"
      }
    ],
    "commands": [
      {
        "command": "templ-project.generic-extended-vscode.applySettings",
        "title": "Apply Generic Extended Extension Pack for Vscode Settings"
      },
      {
        "command": "templ-project.generic-extended-vscode.resetSettings",
        "title": "Reset Generic Extended Extension Pack for Vscode Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}
