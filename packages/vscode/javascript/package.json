{
  "name": "tpl-vscode-javascript",
  "displayName": "JavaScript Extension Pack for Vscode",
  "description": "Essential JavaScript development environment for VSCode - runtime-agnostic tools for modern JavaScript development",
  "version": "0.0.3",
  "publisher": "templ-project",
  "__configHash": "e732f7e0571f31d3e20e9898ea89d1778b64354922b94db6e76626a808469605",
  "repository": {
    "type": "git",
    "url": "https://github.com/templ-project/vscode-extensions",
    "directory": "packages/vscode/javascript"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "javascript",
    "es6",
    "frontend",
    "web",
    "development",
    "linting",
    "formatting",
    "extension-pack",
    "development",
    "vscode",
    "javascript"
  ],
  "extensionPack": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-vscode.js-debug",
    "mgmcdermott.vscode-language-babel",
    "wix.vscode-import-cost",
    "WallabyJs.quokka-vscode",
    "oven.bun-vscode",
    "denoland.vscode-deno"
  ],
  "extensionDependencies": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-vscode.js-debug",
    "mgmcdermott.vscode-language-babel"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+f",
        "command": "prettier.forceFormatDocument",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+e",
        "command": "eslint.executeAutofix",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "templ-project.javascript-vscode.applySettings",
        "title": "Apply JavaScript Extension Pack for Vscode Settings"
      },
      {
        "command": "templ-project.javascript-vscode.resetSettings",
        "title": "Reset JavaScript Extension Pack for Vscode Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  }
}
