{
{{#each snippets}}
  "{{@key}}": {
    "prefix": "{{this.prefix}}",
    "body": [
{{#if (isString this.body)}}
      {{{json this.body}}}
{{else}}
{{#each this.body}}
      {{{json this}}}{{#unless @last}},{{/unless}}
{{/each}}
{{/if}}
    ],
    "description": "{{this.description}}"
  }{{#unless @last}},{{/unless}}
{{/each}}
}
